<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mappls.sdk.demo" >
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />

    <application
        android:name=".MapApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.VectorMapComposeSample" >



        <activity
            android:name=".sample.activities.camera.CameraFeaturesActivity"
            android:exported="false"
            android:label="@string/title_activity_cameraFeatures"
            android:theme="@style/Theme.VectorMapComposeSample" />
        <activity
            android:name=".sample.activities.camera.B_activity"
            android:exported="false"
            android:label="@string/title_activity_cameraFeatures"
            android:theme="@style/Theme.VectorMapComposeSample" />
        <activity
            android:name=".sample.activities.camera.C_Activity"
            android:exported="false"
            android:label="@string/title_activity_cameraFeatures"
            android:theme="@style/Theme.VectorMapComposeSample" />
        <activity
            android:name=".sample.activities.camera.Library_Activity"
            android:exported="false"
            android:label="@string/title_activity_cameraFeatures"
            android:theme="@style/Theme.VectorMapComposeSample" />
        <activity
            android:name=".sample.activities.camera.Jeweleery"
            android:exported="false"
            android:label="@string/title_activity_cameraFeatures"
            android:theme="@style/Theme.VectorMapComposeSample" />
        <activity
            android:name=".sample.activities.camera.DORMITORY"
            android:exported="false"
            android:label="@string/title_activity_cameraFeatures"
            android:theme="@style/Theme.VectorMapComposeSample" />
        <activity
            android:name=".sample.activities.camera.LocationCameraOptionActivity"
            android:exported="false"
            android:label="@string/title_activity_location_camera_option"
            android:theme="@style/Theme.VectorMapComposeSample" />


        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.MyApp" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>